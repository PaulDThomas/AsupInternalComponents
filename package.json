{
  "name": "asupinternalcomponents",
  "description": "Components developed by Paul for clinical table generation",
  "version": "1.6.1",
  "author": "Paul Thomas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulDThomas/asupinternalcomponents.git"
  },
  "bugs": {
    "url": "https://github.com/PaulDThomas/asupinternalcomponents/issues"
  },
  "homepage": "https://github.com/PaulDThomas/asupinternalcomponents#readme",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "dist/cjs/main.js",
  "module": "dist/main.js",
  "types": "dist/asupinternalcomponents.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky",
    "start": "rmdir /q/s .\\.parcel-cache && parcel demo/index.html",
    "test:watch": "jest --watch",
    "test": "jest",
    "build": "rmdir /q/s .\\.parcel-cache && parcel build src/main.ts"
  },
  "dependencies": {
    "@asup/context-menu": "^1.3.5",
    "@asup/editor-v3": "^1.2.6",
    "draft-js": "^0.11.7",
    "lodash": "^4.17.21",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-rnd": "^10.4.1",
    "uuid": "^9.0.1"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.12.0",
    "@parcel/packager-ts": "^2.12.0",
    "@parcel/transformer-sass": "^2.12.0",
    "@parcel/transformer-typescript-types": "^2.12.0",
    "@parcel/transformer-webmanifest": "^2.12.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/css-modules": "^1.0.5",
    "@types/draft-js": "^0.11.9",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.72",
    "@types/react-dom": "^18.2.22",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.2.2",
    "parcel": "^2.12.0",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "process": "^0.11.10",
    "react-router-dom": "^6.2.1",
    "ts-jest": "^29.1.2",
    "ts-jest-mock-import-meta": "^1.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2",
    "typescript-plugin-css-modules": "^5.1.0"
  },
  "@parcel/transformer-js": {
    "inlineEnvironment": [
      "REACT_APP_*",
      "npm_package_*"
    ]
  },
  "@parcel/transformer-css": {
    "cssModules": {
      "pattern": "aic-[name]-[hash]-[local]",
      "dashedIndents": true
    }
  },
  "staticFiles": [
    {
      "staticPath": "demo/public/static",
      "staticOutPath": "public",
      "env": {
        "NODE_ENV": "development"
      }
    }
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
